@article{BEVINS2019,
	title = "Performance evaluation of an energy tuning assembly for neutron spectral shaping",
	journal = "Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment",
	year = "2019",
	issn = "0168-9002",
	doi = "https://doi.org/10.1016/j.nima.2019.01.049",
	author = "James E. Bevins and Z. Sweger and N. Munshi and B.L. Goldblum and J.A. Brown and D.L. Bleuel and L.A. Bernstein and R.N. Slaybaugh",
	keywords = "Neutron energy tuning, Beam shaping, Neutron activation analysis, Spectrum unfolding, Fast neutrons, Surrogate debris",
	abstract = "An energy tuning assembly (ETA) was designed to be fielded at the National Ignition Facility (NIF) to modify the characteristic D-T fusion spectrum to include a prompt fission neutron spectral component. The ETA was characterized at the 88-Inch Cyclotron at Lawrence Berkeley National Laboratory to measure the shaped spectrum from an incident deuteron breakup neutron source, test the proposed neutron spectroscopy techniques used to inform the flux measurements at NIF, and validate the ability to predict ETA performance using a Monte Carlo Neutral Particle (MCNP) simulation. Activation foils (i.e., Ni, In, Au, Al) were exposed to a collimated 33-MeV deuteron-breakup beam originating from a tantalum breakup target. The source spectrum absent the ETA was characterized using a set of activation foils and the STAYSL unfolding code. Finally, the ETA-modified spectrum was obtained using activation foil unfolding with a œá2ŒΩ=1.32. The ETA-modified unfolded spectrum agreed with the MCNP-simulated prediction in the energy range of 0.1‚Äì14 MeV, but exhibited disagreements in the 10 eV‚Äì100 keV region. This work demonstrates shaping of the NIF neutron spectrum via the ETA to be a viable path forward for tailored neutron beams at NIF."
}
